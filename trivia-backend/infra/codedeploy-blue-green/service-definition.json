{
    "taskDefinition": "trivia-backend",
    "cluster": "default",
    "loadBalancers": [
        {
            "targetGroupArn": "PLACEHOLDER_TARGET_GROUP",
            "containerName": "web",
            "containerPort": 80
        }
    ],
    "desiredCount": 3,
    "launchType": "FARGATE",
    "schedulingStrategy": "REPLICA",
    "deploymentController": {
        "type": "CODE_DEPLOY"
    },
    "networkConfiguration": {
        "awsvpcConfiguration": {
            "subnets": [
                "PLACEHOLDER_SUBNET"
            ],
            "securityGroups": [
                "PLACEHOLDER_SECURITY_GROUP"
            ],
            "assignPublicIp": "DISABLED"
        }
    }
}
