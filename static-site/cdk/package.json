{
  "name": "trivia-game-static-site-infrastructure",
  "version": "0.1.0",
  "main": "bin/index.js",
  "types": "bin/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "synth-infra": "cdk synth -o build --app 'node infrastructure.js'",
    "deploy-infra-dev": "cdk deploy --app 'node infrastructure.js' TriviaGameStaticSiteInfraTest",
    "deploy-infra-prod": "cdk deploy --app 'node infrastructure.js' TriviaGameStaticSiteInfraProd"
  },
  "devDependencies": {
    "@types/node": "^10.9.4",
    "aws-cdk": "^1.7.0",
    "typescript": "^3.1.2"
  },
  "dependencies": {
    "@aws-cdk/aws-certificatemanager": "^1.7.0",
    "@aws-cdk/aws-cloudfront": "^1.7.0",
    "@aws-cdk/aws-route53": "^1.7.0",
    "@aws-cdk/aws-route53-targets": "^1.7.0",
    "@aws-cdk/aws-s3": "^1.7.0",
    "@aws-cdk/aws-s3-deployment": "^1.7.0",
    "@aws-cdk/core": "^1.7.0"
  }
}
